{
    "env": {
        "es2021": true,
        "jest/globals": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:node/recommended", "plugin:markdown/recommended", "prettier"],
    "overrides": [],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "babelOptions": {
            "plugins": ["@babel/plugin-syntax-import-assertions"]
        },
        "ecmaVersion": "latest",
        "requireConfigFile": false,
        "sourceType": "module"
    },
    "plugins": ["jest", "markdown"],
    "rules": {
        "no-console": 1,
        "no-unused-vars": 1,
        "node/no-extraneous-import": 1,
        "node/shebang": 0
    }
}
