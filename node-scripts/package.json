{
    "author": "Jurand Niemczycki <createdbyjurand@gmail.com>",
    "bin": {
        "rebuild-modules@createdbyjurand": "./bin/rebuild-modules.node.mjs",
        "reinstall-dependencies@createdbyjurand": "./bin/reinstall-dependencies.node.mjs",
        "update-release-version-number@createdbyjurand": "./bin/update-release-version-number.node.mjs"
    },
    "description": "@createdbyjurand/node-scripts",
    "devDependencies": {
        "@prettier/plugin-xml": "^2.2.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-markdown": "^3.0.0",
        "eslint-plugin-node": "^11.1.0",
        "jest": "^29.4.1",
        "prettier": "^2.8.3",
        "prettier-plugin-java": "^2.0.0",
        "prettier-plugin-properties": "^0.2.0",
        "prettier-plugin-sh": "^0.12.8"
    },
    "license": "ISC",
    "main": "index.node.mjs",
    "name": "@createdbyjurand/node-scripts",
    "scripts": {
        "publish": "npm publish --access public",
        "rebuild @shared module": "node bin/rebuild-modules.node.mjs --path=@shared",
        "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --config jest.config.mjs",
        "update dependencies": "npm i --save-dev @prettier/plugin-xml@latest cross-env@latest eslint-config-prettier@latest eslint-plugin-jest@latest eslint-plugin-markdown@latest eslint-plugin-node@latest eslint@latest jest@latest prettier-plugin-java@latest prettier-plugin-properties@latest prettier-plugin-sh@latest prettier@latest",
        "update release version number": "node bin/update-release-version-number.node.mjs"
    },
    "version": "0.5.3-SNAPSHOT"
}
